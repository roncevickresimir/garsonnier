{
  "compilerOptions": {
      "baseUrl": "./src",
      "importHelpers": true,
      "inlineSourceMap": true,
      "module": "CommonJS",
      "noEmitHelpers": true,
      "outDir": "dist",
      "preserveConstEnums": true,
      "skipLibCheck": true,
      "esModuleInterop": true,
      "strictNullChecks": true,
      "target": "ES2022"
  }
}
